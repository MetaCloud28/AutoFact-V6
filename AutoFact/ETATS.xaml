<Page x:Class="AutoFact.ETATS"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AutoFact"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ETATS">

    <Grid>
        <Border Background="#2F2E2A" CornerRadius="15">
            <Label x:Name="ETATS_label" Content="ETATS" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="36" Foreground="White" Margin="10,0,0,0"/>
        </Border>
        <ListView x:Name="listViewClients" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="258,115,258,69">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Page>
